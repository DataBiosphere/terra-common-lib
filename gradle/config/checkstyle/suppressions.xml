<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
    "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
  <!--
    Terra: final is considered optional for local vars.
  -->
  <suppress checks="FinalLocalVariable"/>
  <!--
    Terra: Spotless is canonical for indentation.
  -->
  <suppress checks="Indentation"/>
  <!--
    Terra: inner types out of order is OK, especially in tests.
  -->
  <suppress checks="InnerTypeLast"/>
  <!--
    Terra: less strict about javadoc.
  -->
  <suppress checks="JavadocType"/>
  <!--
    Terra: there are legitimate cases (esp. in tests) where multiple string literals are okay.
  -->
  <suppress checks="MultipleStringLiterals"/>

</suppressions>